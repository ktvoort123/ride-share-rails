<h1>Passengers</h1>

<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Phone Number</th>
  </tr>

  <% @passengers.each do |passenger|  %>
    <tr>
      <td><%= passenger.id %></td>
      <td><%= link_to passenger.name, passenger_path(passenger.id) %></td>
      <td><%= passenger.phone_num %></td>
    </tr>
  <% end %>
</table>