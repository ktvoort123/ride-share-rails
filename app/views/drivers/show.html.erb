<h1><%=@driver.name%></h1>
<p><%=@driver.vin%></p>
<p><%=@driver.trips%></p>

  <% @driver.trips.each do |trip|  %>
    <li>
      <p>ID: <%=trip.id%></p>
      <p>Date: <%=trip.date%></p>
      <p>Rating: <%=trip.rating%></p>
      <p>Cost: <%=trip.cost%></p>
      <p>Passenger: <%=trip.passenger.name%></p> 
      <p>Driver: <%=trip.driver.name%></p>
    </li> 
  <% end %>
